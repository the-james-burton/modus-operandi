    <!--  timer to refresh task list -->
    <bean id="refreshProcessList" class="org.springframework.scheduling.timer.ScheduledTimerTask">
        <property name="delay" value="0" />
        <property name="period" value="10000" />
        <property name="runnable" ref="services" />
    </bean>

    <bean id="refreshProperties" class="org.springframework.scheduling.timer.ScheduledTimerTask">
        <property name="delay" value="0" />
        <property name="period" value="50000" />
        <property name="runnable" ref="propertiesLoader" />
    </bean>

    <bean id="timerFactory" class="org.springframework.scheduling.timer.TimerFactoryBean">
        <property name="scheduledTimerTasks">
            <list>
                <ref bean="refreshProcessList" />
                <ref bean="refreshProperties" />
            </list>
        </property>
    </bean>

